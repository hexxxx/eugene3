<div class="row">
  <div class="col-md-12">
    <%= simple_form_for @protein, html: { class: 'form-horizontal' } do |form| %>
      <%= form.input :name %>
      <%= form.input :sequence %>
      <%= form.submit class: "btn btn-primary" %>
    <% end %>
    <!-- This will write the sequence and name to the database. I can then use an instance variable, or call that protein by name or ID, and then work with the sequence. -->
  </div>
</div>
